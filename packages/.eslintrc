{
  "root": false,
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "project": ["./tsconfig.json"] },
  "extends": ["leap"],
  "env": { "es2017": true },
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_", "caughtErrors": "none" }
    ]
  }
}
