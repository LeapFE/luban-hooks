{
  "name": "@luban-hooks/use-request",
  "version": "0.0.1",
  "description": "React Hooks for fetching data, based on axios",
  "keywords": [
    "hooks",
    "react",
    "axios"
  ],
  "author": "front-end-captain <18810940932@163.com>",
  "homepage": "https://github.com/LeapFE/luban-hooks#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib/",
    "es/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeapFE/luban-hooks.git"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --color",
    "test:update": "jest -u",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "check-type": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "clean": "rimraf es && rimraf lib",
    "pre:release": "npm run clean && npm run format && npm run eslint",
    "prepublishOnly": "npm run build",
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "rimraf es && cross-env NODE_ENV=production tsc --module es2015 --outDir es",
    "build:cjs": "rimraf lib && cross-env NODE_ENV=production tsc --module commonjs --outDir lib"
  },
  "bugs": {
    "url": "https://github.com/LeapFE/luban-hooks/issues"
  },
  "peerDependencies": {
    "axios": "0.19.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4"
  }
}
